## ECHO CONFIGURATION FOR SEMPLICE (NOLIVE VERSION)

[linstaller:extends]
source = semplice-persistent

#[module:echo.partusb]
# This only works if using the mount_nolive.sh script
#image = /linstaller/source/live/filesystem.squashfs

